{
  "name": "SLN-One-Click",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "storage", "alarms"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "reguw_logo.png"
  },
  "background": {
    "service_worker": "./scripts/background.js"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://register.uw.edu/*", "https://sdb.admin.uw.edu/*"]
    }
  ]
}
